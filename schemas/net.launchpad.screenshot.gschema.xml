<schemalist>
	<schema id="net.launchpad.screenshot" path="/net/launchpad/screenshot/" gettext-domain="screenshot">      
        <key name="format" type="s">
            <default>"png"</default>
        </key>        
        <key name="mouse-pointer" type="b">
            <default>false</default>
        </key>
        <key name="include-date" type="b">
            <default>true</default>
        </key>
        <key name="close-on-save" type="b">
            <default>false</default>
        </key>
        <key name="folder-dir" type="s">
            <default>""</default>
        </key>
	</schema>
</schemalist>
